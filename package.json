{
    "name": "vue.vtaiwan.tw",
    "version": "1.0.0",
    "description": "A Vue.js project",
    "author": "Ly <lyforever62@hotmail.com>",
    "private": true,
    "scripts": {
        "dev": "node server",
        "start": "cross-env NODE_ENV=production node server",
        "build": "npm run build:client && npm run build:server",
        "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
        "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
        "build:static": "cross-env NODE_ENV=production webpack --config build/webpack.static.config.js --progress --hide-modules && cp 404.html dist"
    },
    "engines": {
        "node": ">=6.9.0"
    },
    "dependencies": {
        "autoprefixer": "^6.4.0",
        "axios": "^0.15.3",
        "babel-core": "^6.0.0",
        "babel-loader": "^6.0.0",
        "babel-preset-es2015": "^6.13.2",
        "babel-preset-stage-2": "^6.11.0",
        "cross-env": "^2.0.0",
        "css-loader": "^0.25.0",
        "es6-promise": "^3.2.1",
        "express": "^4.14.0",
        "extract-text-webpack-plugin": "^2.0.0-beta.3",
        "favicons-webpack-plugin": "^0.0.7",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^2.26.0",
        "jade": "^1.11.0",
        "livescript": "^1.5.0",
        "lru-cache": "^4.0.1",
        "node-bourbon": "^4.2.8",
        "node-sass": "^3.10.1",
        "prerender-spa-plugin": "^2.0.1",
        "sass-loader": "^4.0.2",
        "serialize-javascript": "^1.3.0",
        "serve-favicon": "^2.3.0",
        "string-natural-compare": "^2.0.2",
        "stylus": "^0.54.5",
        "stylus-loader": "^2.1.2",
        "url-loader": "^0.5.7",
        "vue": "^2.0.0",
        "vue-loader": "^9.5.0",
        "vue-router": "^2.0.0",
        "vue-server-renderer": "^2.0.0",
        "vuex": "^2.0.0",
        "vuex-router-sync": "^3.0.0",
        "webpack": "^2.1.0-beta.20",
        "webpack-dev-middleware": "^1.6.1",
        "webpack-hot-middleware": "^2.12.2"
    }
}
